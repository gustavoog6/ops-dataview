{
  "name": "ops-dataview-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && cross-env VITE_MODE=production vite build",
    "build:app": "tsc -b && vite build --mode app",
    "build:app-checkout": "tsc -b && vite build --mode checkout",
    "build:app-new-order": "tsc -b && vite build --mode new-order",
    "build:app-product-detail": "tsc -b && vite build --mode product-detail",
    "build:app-product-detail-actions": "tsc -b && vite build --mode product-detail-actions",
    "build:app-products-list": "tsc -b && vite build --mode products-list",
    "build:app-products-list-item": "tsc -b && vite build --mode products-list-item",
    "build:app-screen": "tsc -b && vite build --mode screen",
    "build:app-search": "tsc -b && vite build --mode search",
    "build:app-product-search-results": "tsc -b && vite build --mode product-search-results",
    "test": "jest --ci --collectCoverage=false",
    "report": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --collectCoverage=false",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src/**/*.{jsx,ts,tsx}",
    "format": "prettier --write \"**/*.*\""
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/lab": "5.0.0-alpha.173",
    "@mui/material": "5.16.7",
    "@mui/x-data-grid-premium": "7.14.0",
    "@mui/x-date-pickers-pro": "7.14.0",
    "@zydon/auth": "^2.0.7",
    "@zydon/common": "^2.6.93",
    "@zydon/plugin": "^1.0.34",
    "jest-fetch-mock": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.26.3",
    "@originjs/vite-plugin-federation": "1.3.5",
    "@testing-library/dom": "10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "jest-junit": {
    "outputDirectory": "./reports",
    "outputName": "junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
